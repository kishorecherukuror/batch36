<h1>Products#list</h1>

<table border="1">
<tr>
	<th>Name</th>
	<th>Price</th>
	<th>Quantity</th>
	<th>Actions</th>
</tr>
<% @products.each do |p| %>
<tr>
	<td><%= p.name %></td>
	<td><%= p.price %></td>
	<td><%= p.quatity %></td>
	<td><%= link_to "show", products_path(p) %>
	<%= link_to "edit", products_edit_path(p) %>
	<%= link_to "delete", products_destroy_path(p), method: 'delete',  data: {confirm: "Are you sure?"} %>
	</td>
</tr>
<% end %>
